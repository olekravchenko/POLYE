 declare    circl f1; 
 omega      w = f1;   
 function   g1= w;    
    w1 = -(-g1 & y);
    w2 =    g1 ! y;
    u  = (w1 + w2 * sqrt(x*x+g1)) / (w1 + w2);
 program
    result(s1,fu);    
    fu(u)=u;
 value
    const =  1,1,1,1,10,1,1;
    tabl  =  1,1,1,1,1, 0,1,1,0,0;
    f1    =  0,0,1;   
    s1    = 20, -1,-1,1,1 ;
end