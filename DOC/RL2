                38   FU(U)=P1;FV(U)=P2;
                39   FUV(U)=P3;
                40 VALUE
                41    CONST=2,1,1,3,72,1,1;
                42    TABL=NU,2,1,2,2,NV,1,2,2,2,
                                   0,1,2,0,0;
                43   NU= 5;
