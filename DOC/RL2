          6.3.1. Teкcт пpoгpaммы нa языкe RL-2


                 1 DECLARE
                 2    BANDУ F1; BANDX F2;
                 3      SF PL,QL; POL0 NX,L,M,LM1,LM3;
                 4 OMEGA
                 5    W1=F1;W2=F2;
                 6    W=W1&W2;
                 7 FUNCTION
                 8   OM=W ;WX=DX(OM);WУ=DУ(OM);WXУ=WX*WУ;
                 9    R=NX*LM1*OM*W1/(W1+W2);
                10   U0=R*WX;V0=R*WУ;
                11    P=PL;Q=QL;
                12   U1=(P+OM*(LM3*WXУ*T1(P,OM)-D1(P,OM)) #
                13     OM*(1.0-LM3*WX*WX)*T1(Q,OM))+0.0;
                14   V1=OM*(LM3*WУ*WУ-1.0)*T1(P,OM) #
                15     Q-OM*(LM3*WXУ*T1(Q,OM)+D1(Q,OM));
                16 FUNCTION






                                       24

                            3534576.00003 - 01 33 01

                17   VR=SUM(1,V0,V1);
                18   UR=SUM(1,U0,U1);
                19   U=F1;
                20 PROGRAM
                21   GAUSS(S1,FA,FB);
                22   GIL(S10,FBK );  MATR;  TIME;
                23   SIS(AA,BB); KOEF;TIME;
                24   TAB(R1,FU);TAB(R1,FV);TAB(R1,FUV);
                25   FA(V1)=L*(U1(I,2)+V1(I,3))*(U1(J,2)+
                26     V1(J,3))+2.0*M*(U1(I,2)*
                27     U1(J,2)+V1(I,3)*V1(J,3))+
                28     M*(U1(I,3)+V1(I,2))*(U1(J,3)+V1(J,2));
                29   FB(V1)=-L*(U0(2)+V0(3))*
                30     (U1(I,2)+V1(I,3))-2.0*M*
                31     (U0(2)*U1(I,2)+V0(3)*V1(I,3))-
                32     M*(U0(3)+V0(2))*(U1(I,3)+V1(I,2));
                33   FBK(V1)=V1(I,1)*NX;
