          6.1.1. Teкcт пpoгpaммы нa языкe RL-2


                 1 DECLARE
                 2 LINE F2,F3;
                 3 BANDУ F1;
                 4 MBANDУ  F4; MBANDX F5;
                 5 SF P;
                 6 CORTEG OM,R,U0;
                 7 CORTEG G136,G245,G24,G13,G16;
                 8 MODUL O,U1;
                 9 MODUL U;
                10 ARRAУ S1; ARRAУ R1;
                11 ARRAУ R2;
                12 POL1 F6,F7,A1;
                13 END

                14 FUNCTION
                15 OM = O;
                16 R = PASTE(G136,G16,0.0,A1);
                17 U0 = PASTE(G13,G245,R,100.0);
                18 U1 = OM * P;
                19 END

                20 OMEGA
                21 G13 = F1 & F3;
                22 G136 = G13 ! F6;
                23 G16 = F1 ! F7;
                24 G24 = F4 ! F5;
                25 G245 = G24 & F2;
                26 O = G13 & F2 & G24;
                27 END

                28 FUNCTION
                29 U = SUM(1,U0,U1);
                30 END

                31 PROGRAM
                32 GAUSS(S1,FA,FB);
                33 SIS(A,B);
                34 ATS;
                35 TAB(R1,FU);
                36 PIC(R2,FU);
                37 A=A1(1);
                38 B=B1(1);
                39 FA(U1)=(10.+У)*(U1(I,2)*U1(J,2)+
                                   U1(I,3)*U1(J,3));
                40 FB(U1)=-(10.+У)*(U0(2)*U1(I,2)+
                                    U0(3)*U1(I,3));
                41 FU(U)=U;
                42 END

                43 VALUE
                44 CONST = 1,1,1,2,55,1,1;
                45 TABL = M,1,1,2,1, 0,1,1,0,0;
                46 M = 11;






                                       17

                            3534576.00003 - 01 33 01

                47 P = 1,ПP;
                48 ПP = -30,0,30,60;
                49 F1 = 0,30;
                50 F4 = 0,12;
                51 F5 = 30,12;
                52 F2 = 30,60,-30,60;
                53 F3 = -30,0,30,0;
                54 F6 = -30,0,1;
                55 F7 = 30,0,-1;
                56 A1 = -100,0,3.333333;
                57 S1 = K,0,42,12,60, 12,42,30,60, 12,18,30,30,
